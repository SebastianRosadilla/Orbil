{
  "name": "Orbil-Back",
  "version": "0.1.0",
  "author": "Alfredo Sebastian Rosadilla Ribeiro",
  "license": "MIT",
  "description": "Orbil backend",
  "scripts": {
    "clean": "rm -rf build node_modules",
    "start": "tsc && node ./build/www.js",
    "watch": "tsc && DEBUG=nodetyped:* concurrently -k 'tsc --watch' 'nodemon --delay 2000ms'",
    "loadMocks": "tsc && node ./build/tasks/loadMocks.js",
    "lint": "tsconfig-lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SebastianRosadilla/Orbil"
  },
  "keywords": [
    "node.js",
    "typescript",
    "mongoose",
    "express",
    "async", "await",
    "scss",
    "ejs",
    "nodemon",
    "bootstrap",
    "tslint"
  ],
  "main": "build/www.js",
  "dependencies": {
    "@types/body-parser": "0.0.32",
    "@types/express": "^4.0.32",
    "@types/node": "^6.0.38",
    "body-parser": "^1.15.2",
    "bootstrap": "^4.0.0-alpha.2",
    "cookie-parser": "^1.4.3",
    "debug": "^2.2.0",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "jquery": "^3.1.0",
    "mongoose": "^4.6.0",
    "morgan": "^1.7.0",
    "node-sass-middleware": "^0.9.8",
    "serve-favicon": "^2.3.0",
    "tether": "^1.3.7"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "tsconfig-lint": "^0.11.0",
    "tslint": "^3.15.1",
    "typescript": "^2.0.2"
  }
}
